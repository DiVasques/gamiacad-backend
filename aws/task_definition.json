{
    "family": "gamiacad-api",
    "containerDefinitions": [
        {
            "name": "gamiacad-api",
            "image": "",
            "cpu": 2048,
            "memory": 7168,
            "memoryReservation": 6144,
            "portMappings": [
                {
                    "name": "gamiacad-api-4000-tcp",
                    "containerPort": 4000,
                    "hostPort": 4000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/gamiacad-api",
                    "awslogs-region": "sa-east-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
    ],
    "executionRoleArn": "arn:aws:iam::307231294774:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "requiresCompatibilities": [
        "EC2"
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}